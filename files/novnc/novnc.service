[Unit]
Description="noVNC"
After=multi-user.target vncserver-x11-serviced.service

[Service]
User=astroid
ExecStart=/usr/bin/websockify --log-file=/var/log/astroid/novnc.log --web=/var/www/novnc/ 8080 localhost:5900
ExecStop=/usr/bin/pkill websockify
Restart=on-failure
RestartSec=3

[Install]
WantedBy=multi-user.target

